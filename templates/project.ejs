<div class="bg-black bg-opacity-25 p-8 text-2xl gap-3 flex flex-col rounded-2xl h-[100%]">
    <h2 class="font-bold text-3xl"><%= project.name %></h2>
    <p class="marked">
        <%- project.description %>
    </p>
    <div class="flex gap-3 mt-auto">
    <% project.links.forEach(function(link) { %>
        <a href="<%= link.url %>" target="_blank" class="border rounded-md w-fit border-aqua-self p-2 flex gap-3 text-lg items-center text-aqua-self link">
            <%= link.text %>
            <i svg="" class="text-aqua-self align-middle">
                <%- ICONS['upright'] %>
            </i>
        </a>
    <% }); %>
    </div> 
</div>